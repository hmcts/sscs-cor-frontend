{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-standard"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "semicolon": [true, "always"],
        "space-before-function-paren": false,
        "strict-type-predicates": false,
        "no-console": true
    },
    "rulesDirectory": []
}